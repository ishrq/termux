snippet fun
    function ${1: func_name}
        ${2}
    end

    ${0}

snippet switch
    switch \$${1: arg1}
        case '${2: case}'
            ${3}
    end

    ${0}

snippet case
    case '${1: case}'
        ${0}

snippet if
    if ${1: condition}
        ${2}
    end

snippet set
    set ${1: var} '${2: value}'${0}
